		</if>
		<if test="allDueStatus!=null and allDueStatus!=''">
			ALL_DUE_STATUS=#{allDueStatus},
		</if>
		<if test="lowDueStatus!=null and lowDueStatus!=''">
			LOW_DUE_STATUS=#{lowDueStatus},
		</if>
		<if test="periods!=null and periods!=''">
